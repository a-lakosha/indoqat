<odoo>
    <template id="header_call_to_action_inherited"
              inherit_id="website.header_call_to_action"
              name="Header Call to Action"
              active="True">
        <xpath expr="//a" position="replace">
            <a href="/get-start"
               class="oe_unremovable btn fw-bold btn-primary btn_cta"
               style="color: white !important;"
               aria-label="Get Started">
                Get Started
            </a>
        </xpath>
    </template>

    <template id="partners_templates">
        <owl-component name="theme_taqatechno.partners"/>
    </template>
</odoo>
